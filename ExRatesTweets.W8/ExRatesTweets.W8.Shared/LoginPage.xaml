<Page
    x:Class="ExRatesTweets.W8.LoginPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ExRatesTweets.W8"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <WebView x:Name="webView" HorizontalAlignment="Center" VerticalAlignment="Center" Height="459" Margin="156,59,277,250" Width="933" />
        <Button x:Name="signinBtn" Content="Zaloguj się do TT" VerticalAlignment="Center" HorizontalAlignment="Center" Click="signinBtn_Click" Margin="619,669,598,61"/>
        <Grid HorizontalAlignment="Left" Height="65" Margin="577,602,0,0" VerticalAlignment="Top" Width="228">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="47*"/>
                <ColumnDefinition Width="10*"/>
            </Grid.ColumnDefinitions>
            <TextBox x:Name="pinTextBox" HorizontalAlignment="Left" Height="39" Margin="0,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="188"/>
            <Button x:Name="confirmPinBtn" Content="Button" Grid.Column="1" HorizontalAlignment="Left" Margin="7,7,0,0" VerticalAlignment="Top" Height="45" Width="33" Click="confirmPinBtn_Click"/>
        </Grid>

    </Grid>
</Page>
